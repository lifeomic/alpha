{
  "name": "@lifeomic/alpha",
  "version": "1.1.0",
  "description": "Unified client for HTTP services.",
  "main": "src/index.js",
  "types": "src/index.ts",
  "author": "LifeOmic <development@lifeomic.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeomic/alpha.git"
  },
  "scripts": {
    "buildGrammar": "nearleyc src/utils/lambdaURL.ne -o src/utils/lambdaURLGrammar.ts",
    "prepare": "yarn buildGrammar",
    "build": "tsc",
    "pretest": "yarn buildGrammar",
    "test": "nyc ava --verbose"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "failWithoutAssertions": false,
    "files": [
      "test/**/*.test.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "./test/setup.ts"
    ]
  },
  "eslintConfig": {
    "extends": "plugin:@lifeomic/node/recommended"
  },
  "eslintIgnore": [
    "node_modules/",
    "coverage/",
    "src/utils/lambdaURLGrammar.ts"
  ],
  "nyc": {
    "all": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "report-dir": "./coverage"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.36",
    "@types/lodash": "^4.14.149",
    "@types/nearley": "^2.11.0",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^7.5.1",
    "@types/url-parse": "^1.4.3",
    "ava": "^2.4.0",
    "aws-sdk-mock": "^4.0.0",
    "eslint": "^6.3.0",
    "nock": "^11.3.3",
    "nyc": "^14.1.1",
    "proxyquire": "^2.0.1",
    "sinon": "^7.2.3",
    "ts-node": "^8.5.4",
    "ts-sinon": "^1.0.24",
    "tsc": "^1.20150623.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "aws-sdk": "^2.184.0",
    "axios": "axios/axios#a17c70cb5ae4acd7aa307b7f7dc869953dea22c4",
    "docker-lambda": "^0.15.3",
    "lodash": "^4.17.11",
    "nearley": "^2.16.0",
    "resolve-pathname": "^3.0.0",
    "url-parse": "^1.4.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
